{% extends 'base.html.twig' %}

{% block title %}Categoriee index{% endblock %}

{% block body %}
    <h1>Categoriee index</h1>

    <table class="table">
        <thead>
            <tr>

                <th>Numero</th>
                <th>Lien</th>
                <th>Description</th>
                <th>Titre</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categoriee in categoriees %}
            <tr>

                <td>{{ categoriee.numero }}</td>
                <td>{{ categoriee.lien }}</td>
                <td>{{ categoriee.description }}</td>
                <td>{{ categoriee.titre }}</td>
                <td>
                    <a href="{{ path('categoriee_show', {'id': categoriee.id}) }}">show</a>
                    <a href="{{ path('categoriee_edit', {'id': categoriee.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('categoriee_new') }}">Create new</a>
{% endblock %}
