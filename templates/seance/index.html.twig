{% extends 'base.html.twig' %}



{% block title %}Seance index{% endblock %}

{% block body %}

    <h1>Seance index</h1>
    <form method="post" action="{{ path('recherche') }}">
        <label>Recherche</label>
        <input id="myInput" type="text" placeholder="Search..">



         </form>


    <table class="table">
        <thead>
            <tr>

                <th>Num Formation</th>
                <th>Lien</th>
                <th>Description</th>
                <th>DateSeance</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody id="myTable">

        {% for seance in seances %}

            <tr>

                <td>{{ seance.idFormation }}</td>
                <td>{{ seance.lien }}</td>
                <td>{{ seance.description }}</td>
                <td>{{ seance.dateSeance }}</td>
                <td>
                    <a href="{{ path('seance_show', {'idSeance': seance.idSeance}) }}">show</a>
                    <a href="{{ path('seance_edit', {'idSeance': seance.idSeance}) }}">edit</a>
                </td>

            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('seance_new') }}">Create new</a>
    {{ knp_pagination_render(seances) }}
{% endblock %}
